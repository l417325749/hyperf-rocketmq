{
  "name": "hyperf/rocketmq",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf"
  ],
  "description": "",
  "autoload": {
    "psr-4": {
      "Hyperf\\RocketMQ\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HyperfTest\\": "tests"
    }
  },
  "require": {
    "php": ">=7.4",
    "hyperf/di": "^2.2 || ^3.0",
    "hyperf/event": "^2.2 || ^3.0",
    "hyperf/guzzle": "^2.2 || ^3.0",
    "hyperf/pool": "^2.2 || ^3.0",
    "hyperf/process": "^2.2 || ^3.0",
    "hyperf/server": "^2.2 || ^3.0",
    "hyperf/utils": "^2.2 || ^3.0",
    "hyperf/logger": "^2.2 || ^3.0",
    "hyperf/framework": "^2.2 || ^3.0",
    "ext-json": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^1.0",
    "swoole/ide-helper": "^4.5"
  },
  "suggest": {
    "swow/swow": "Required to create swow components."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit -c phpunit.xml --colors=always",
    "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
    "cs-fix": "php-cs-fixer fix $1"
  },
  "extra": {
    "hyperf": {
      "config": "Hyperf\\RocketMQ\\ConfigProvider"
    }
  }
}
